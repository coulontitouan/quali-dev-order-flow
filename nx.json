{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "production": [
      "default",
      "!{projectRoot}/src/test/**/*"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/.gitlab-ci.yml"
    ]
  },
  "neverConnectToCloud": true
}
